<% include header %>
<table class="table table-hover">
   <thead>
   <th class="col-sm-3">用户名</th>
   <th class="col-sm-3">电话号码</th>
   <th class="col-sm-3">电子邮箱</th>
   <th class="col-sm-1"></th>
   <th class="col-sm-3"></th>
   </thead>
   <tbody>
   <% docs.forEach(function (doc) {%>
   <tr>
      <td><%= doc.username %></td>
      <td><%= doc.tel %></td>
      <td><%= doc.email %></td>

   <td class="col-sm-1">
      <a href="/edit/<%= doc.username %>/<%= doc.tel %>/<%= doc.email %>"><button class="btn">修改</button></a>
   </td>
   <td class="col-sm-1">
      <a href="/remove/<%= doc.username %>/<%= doc.tel %>/<%= doc.email %>"><button class="btn" onclick="delete">删除</button></a>
   </td>
   </tr>
   <% }) %>
   </tbody>
</table>
<% include paging %>
<% include footer %>